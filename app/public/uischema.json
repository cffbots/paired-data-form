{
  "version": {
    "ui:widget": "hidden"
  },
  "personal": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "ObjectField",
      "collapsed": false
    }
  },
  "metabolomics": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "ObjectField",
      "collapsed": true
    },
    "project": {
      "ui:widget": "radio"
    }
  },
  "genomes": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "table",
      "collapsed": true
    },
    "ui:options": {
      "orderable": false
    },
    "items": {
      "genome_ID": {
        "genome_type": {
          "ui:widget": "radio"
        },
        "GenBank_accession": {
          "ui:placeholder": "At least one accession/ID should be entered"
        },
        "RefSeq_accession": {
          "ui:placeholder": "At least one accession/ID should be entered"
        },
        "ENA_NCBI_accession": {
          "ui:placeholder": "At least one accession/ID should be entered"
        },
        "MGnify_accession": {
          "ui:placeholder": "At least one accession/ID should be entered"
        },
        "JGI_Genome_ID": {
          "ui:placeholder": "At least one accession/ID should be entered"
        },
        "JGI_ID": {
          "ui:placeholder": "At least one accession/ID should be entered"
        }
      },
      "BioSample_accession": {
        "ui:placeholder": "None"
      }
    }
  },
  "proteomes": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "table",
      "collapsed": true
    },
    "ui:options": {
      "orderable": false
    },
    "items": {
      "proteome_ID": {
        "proteome_type": {
          "ui:widget": "radio"
        }
      },
      "method": {
        "analysis_mode": {
          "ui:widget": "radio"
        },
        "peptide_labelling": {
          "ui:widget": "radio"
        }
      },
      "notes": {
        "ui:widget": "textarea",
        "ui:options": {
          "rows": 8
        }
      }
    }
  },
  "experimental": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "ObjectField",
      "collapsed": true
    },
    "sample_preparation": {
      "ui:options": {
        "orderable": false
      },
      "ui:field": "table",
      "items": {
        "medium_details": {
          "medium_type": {
            "ui:widget": "radio"
          },
          "ui:order": [
            "medium_type",
            "medium",
            "Other_medium",
            "Other_medium_link",
            "medium_volume",
            "metagenomic_environment",
            "metagenomic_other_environment",
            "metagenomic_sample_description"
          ]
        },
        "other_growth_conditions": {
          "ui:widget": "textarea",
          "ui:options": {
            "rows": 8
          }
        },
        "metagenomic_sample_description": {
          "ui:widget": "textarea"
        },
        "aeration": {
          "ui:order": [
            "aeration_type",
            "aeration_vessel",
            "aeration_other_vessel",
            "aeration_rpm"
          ]
        }
      }
    },
    "extraction_methods": {
      "ui:options": {
        "orderable": false
      },
      "ui:field": "table",
      "items": {
        "solvents": {
          "ui:options": {
            "orderable": false
          },
          "items": {
            "ui:order": [
              "solvent",
              "Other_solvent",
              "ratio"
            ]
          }
        },
        "extracted_material": {
          "ui:widget": "radio"
        },
        "other_extraction_parameters": {
          "ui:widget": "textarea"
        }
      }
    },
    "instrumentation_methods": {
      "ui:options": {
        "orderable": false
      },
      "ui:field": "table",
      "items": {
        "column": {
          "ui:widget": "radio"
        },
        "mode": {
          "ui:widget": "radio"
        },
        "other_instrumentation": {
          "ui:widget": "textarea"
        }
      }
    }
  },
  "genome_metabolome_links": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "gmtable",
      "collapsed": true
    },
    "ui:options": {
      "orderable": false,
      "widths": {
        "2": "40%"
      }
    },
    "items": {
      "genome_label": {
        "ui:field": "foreignKey",
        "foreignKey": {}
      },
      "proteome_label": {
        "ui:field": "foreignKey",
        "foreignKey": {}
      },
      "sample_preparation_label": {
        "ui:field": "foreignKey",
        "foreignKey": {}
      },
      "extraction_method_label": {
        "ui:field": "foreignKey",
        "foreignKey": {}
      },
      "instrumentation_method_label": {
        "ui:field": "foreignKey",
        "foreignKey": {}
      }
    }
  },
  "BGC_MS2_links": {
    "ui:field": "collapsible",
    "collapse": {
      "field": "ArrayField",
      "collapsed": true
    },
    "ui:options": {
      "orderable": false
    },
    "items": {
      "known_link": {
        "ui:widget": "textarea",
        "ui:options": {
          "rows": 3
        }
      },
      "verification": {
        "ui:widget": "checkboxes"
      },
      "quantitative_experiment": {
        "quantitative_experiment_type": {
          "ui:widget": "radio"
        },
        "quantitative_proteomics_experiment": {
          "difference": {
            "ui:widget": "textarea"
          },
          "proteome_sample1": {
            "protein_fold_changes": {
              "ui:field": "table"
            }
          },
          "proteome_sample2": {
            "protein_fold_changes": {
              "ui:field": "table"
            }
          }
        }
      },
      "BGC_ID": {
        "BGC": {
          "ui:widget": "radio"
        }
      },
      "link": {
        "ui:widget": "radio"
      },
      "MS2_URL": {
        "ui:field": "foreignKey",
        "foreignKey": {}
      }
    }
  }
}
