{
  "name": "podp-api",
  "version": "1.0.0",
  "description": "",
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon -i data dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon -i data --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "async": "^2.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-async-handler": "^1.1.4",
    "lusca": "^1.5.2",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.0",
    "uuid": "^3.3.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/jest": "^24.0.12",
    "@types/lusca": "^1.5.0",
    "@types/node": "^12.0.0",
    "@types/passport": "^0.4.3",
    "@types/passport-http-bearer": "^1.0.32",
    "@types/rimraf": "^2.0.2",
    "@types/uuid": "^3.4.4",
    "concurrently": "^3.5.1",
    "jest": "^24.0.4",
    "nodemon": "^1.13.0",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.1",
    "supertest": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=10"
  }
}
